{
  "name": "nhsuk-react-components",
  "version": "0.6.1",
  "description": "NHS.UK Frontend components ported to React",
  "author": "Tomdango",
  "license": "MIT",
  "repository": "Tomdango/nhsuk-react-components",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "lint": "eslint",
    "deploy-storybook": "yarn build-storybook & storybook-to-ghpages -e storybook-static",
    "build-storybook": "build-storybook -c .storybook -o storybook-static -s ./.storybook/public/",
    "storybook": "start-storybook -p 9001 -c .storybook -s .storybook/public",
    "build": "rollup -c && yarn babel src/ --out-dir lib/",
    "prepare": "yarn run build"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-centered": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/core": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.0.6",
    "@svgr/rollup": "^2.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.4.1",
    "react-scripts": "^2.1.8",
    "react-style-proptype": "^3.2.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.1",
    "storybook-readme": "^5.0.2",
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "nhsuk-frontend": "^2.1.0"
  },
  "files": [
    "dist",
